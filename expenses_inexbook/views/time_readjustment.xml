<odoo>

    <record id="inex_time_readjustment_view_tree" model="ir.ui.view">
        <field name="name">inex_time_readjustment_view_tree</field>
        <field name="model">inex.time.readjustment</field>
        <field name="arch" type="xml">
            <tree string="Time Readjustment" editable="top">
                <field name="name"/>
                <field name="count_days"/>
            </tree>
        </field>
    </record>

    <record id="inex_time_readjustment_action" model="ir.actions.act_window">
        <field name="name">Time Readjustment</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">inex.time.readjustment</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>